
public class insertionSort {
	public static void main(String[] args){
		int a[]={5,8,14,3,12};
		int size=a.length;
		int i,j,temp=0;
		i=1;
		
		while(i<size){
			j=i;
			temp=a[j];
			while(j>0 && a[j-1]>temp){
				a[j]=a[j-1];
				a[j-1]=temp;
				j--;
			}
			i++;
		}
		System.out.println(a[2]);
	}

}
